{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/filter.py"
      },
      {
        "source": "/(.*\\.(?:css|js|map|gif|png|jpg|jpeg|svg|eot|otf|ttf|woff|woff2))",
        "destination": "/public/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/public/index.html"
      }
    ]
}
